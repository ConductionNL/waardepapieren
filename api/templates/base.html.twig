    {#   Sticky menu script   #}
    {% if application['defaultConfiguration']['configuration']['stickyMenu'] is defined and application['defaultConfiguration']['configuration']['stickyMenu'] == true %}
        <script>
            // When the user scrolls the page, execute myFunction
            window.onscroll = function () {
                myFunction()
            };

            // Get the header
            var header = document.getElementById("docs-nav");
            // var stickyMobileNav = document.getElementById("stickyMobileNav");

            // Get the offset position of the navbar
            var sticky = header.offsetTop;

            // var sticky = stickyMobileNav.offsetTop;

            // Add the sticky class to the header when you reach its scroll position. Remove "sticky" when you leave the scroll position
            function myFunction() {
                if (window.pageYOffset > sticky) {
                    header.classList.remove("sticky-transition");
                    header.classList.add("sticky-nav");

                    // stickyMobileNav.classList.remove("sticky-mobile-transition");
                    // stickyMobileNav.classList.add("sticky-mobile-nav");
                } else {
                    header.classList.add("sticky-transition");
                    header.classList.remove("sticky-nav");

                    // stickyMobileNav.classList.add("sticky-mobile-transition");
                    // stickyMobileNav.classList.remove("sticky-mobile-nav");
                }
            }
        </script>
    {% endif %}

    <script>
        // Load collapse component
        System['import']('/uno/components/collapse/collapse.js').then(function (m) {

            //gebruikt unieke id van de nav om te voorkomen dat example collapses ook automatisch geactiveerd worden.
            new m.Collapse(document.querySelector('#search-trigger'));
        });
        // Load nav component
        System.import('/uno/components/navigation/navigation.js').then(function (module) {
            var navs = document.querySelectorAll('.top-nav-autoresize');
            // Initialize all navs
            for (var i = 0; i < navs.length; i++) {
                new module.AutoResizeNavigation(navs.item(i));
            }
            var submenus = document.querySelectorAll('.nav--submenu');
            // Initialize all submenus
            for (var i = 0; i < submenus.length; i++) {
                new module.SubMenuNavigation(submenus.item(i));
            }
        });

        var myModal;
        // Load modal component
        System.import('/uno/components/modal/modal.js').then(function (module) {
            myModal = new module.Modal(document.getElementById('myModal'));
        });

        // Called when the user clicks the button
        function openModal() {
            myModal.open();
        }
    </script>

    <script>
        // Load tooltip component
        System.import('/uno/components/tooltip/tooltip.js').then(function (module) {
            // Select all tooltip elements on the page
            var tooltips = document.querySelectorAll('.example [x-uno-tooltip]');

            // Initialize all tooltips
            for (var i = 0; i < tooltips.length; i++) {
                new module.Tooltip(tooltips.item(i));
            }
        });
    </script>

    {% block script %}{% endblock %}
