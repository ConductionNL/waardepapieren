            /* Sticky docs-nav bar */

            /* The sticky class is added to the header with JS when it reaches its scroll position */
            .sticky-nav {
                position: sticky;
                top: 0px;
                background: var(--menu) !important;
                color: var(--menu-color) !important;
                transition: 0.2s;
                z-index: 1;
            }

